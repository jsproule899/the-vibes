{
  "routes": [
    {
      "route": "/api/{*rest}",
      "allowedRoles": ["anonymous"],
      "proxy": {
        "backendUri": "https://api.thevibes.fun/api/{rest}"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/images/*.{png,jpg,gif}",
      "/css/*",
      "/assets/*",
      "img/*"
    ]
  }
}